package Java;
public class PrintIntDoubleChar {
	 public static void main(String[] args) {
	        int intValue = 24;
	        double doubleValue = 3.14;
	        char charValue = 'A';

	        System.out.println("Integer: " + intValue);
	        System.out.println("Double: " + doubleValue);
	        System.out.println("Character: " + charValue);
	    }
}
